{
  "name": "carbon-prototypes",
  "version": "1.0.0",
  "private": true,
  "description": "Carbon Health Design Prototypes Monorepo",
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "dev": "npm run dev --workspace=clinic-planner",
    "dev:clinic-planner": "npm run dev --workspace=clinic-planner",
    "dev:index": "npm run dev --workspace=prototype-index",
    "dev:ehr": "npm run dev --workspace=ehr-prototype",
    "dev:rn": "npm run start --workspace=react-native-demo",
    "build": "npm run build --workspace=clinic-planner",
    "build:tokens": "npm run build --workspace=@carbon-health/design-tokens",
    "build:icons": "npm run generate:all --workspace=@carbon-health/design-icons",
    "storybook": "npm run storybook --workspace=clinic-planner",
    "storybook:tokens": "npm run storybook --workspace=@carbon-health/design-tokens",
    "lint": "npm run lint --workspaces --if-present",
    "type-check": "npm run type-check --workspaces --if-present",
    "clean": "rm -rf node_modules apps/*/node_modules packages/*/node_modules apps/*/.next packages/*/dist"
  },
  "keywords": [
    "carbon-health",
    "design-system",
    "prototypes",
    "monorepo"
  ],
  "author": "",
  "license": "UNLICENSED",
  "devDependencies": {
    "typescript": "^5.9.2"
  }
}
