{
  "name": "@carbon-health/design-icons",
  "version": "1.0.0",
  "description": "Carbon Health Design Icons - Lucide-based icon system",
  "main": "index.ts",
  "types": "index.ts",
  "exports": {
    ".": "./index.ts",
    "./Icon": "./Icon.tsx",
    "./BicolorIcon": "./BicolorIcon.tsx",
    "./icon-map": "./icon-map.ts",
    "./bicolor-icon-map": "./bicolor-icon-map.ts",
